<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.0</Version>
    <Description>Allows xaml to bind to an expression rather than just properties.</Description>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Keflon/FunctionZero.zBindTestApp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Keflon/FunctionZero.zBindTestApp</RepositoryUrl>
    <PackageTags>Bind Binding Databind Xamarin Forms xaml expression parser evaluator</PackageTags>
    <PackageReleaseNotes>Updated packages to use ExpressionParserZero V4.0.0 to gain access to all the new goodies it supports.</PackageReleaseNotes>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <Company>FunctionZero Ltd</Company>
    <Copyright>Function Zero Ltd</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FunctionZero.ExpressionParserZero" Version="4.0.0" />
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.2012" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
